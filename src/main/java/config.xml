<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">
 
	<bean id="student1" class="com.springCore.SpringCore.Student">
		<property name="StudentID">
			<value>0001</value>
		</property>
		<property name="StudentName" value="Happy Singh"/>			
		
		<property name="StudentAddress">
			<value>Varanasi</value>
		</property>
	</bean>
	
	<!-- second bean -->
	<bean id="student2" class="com.springCore.SpringCore.Student">
		<property name="StudentID" value="002"/>			
		<property name="StudentName" value="kurtika don"/>		
		<property name="StudentAddress" value="America"/>
	</bean>

<!-- Third bean -->
	<bean id="student35" class="com.springCore.SpringCore.Student" name="student3" p:studentID="00003" p:studentName="p Schema name" p:studentAddress="China"/>

	<!-- more bean definitions go here -->
</beans>